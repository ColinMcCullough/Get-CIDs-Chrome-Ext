{
  "manifest_version": 2,

  "name": "Get All GMB CIDs",
  "description": "Execute script after click in popup.html",
  "version": "1.91",

  "icons": {
    "48": "icon.png"
  },

  "permissions": [
    "tabs", 
    "<all_urls>",
    "http://*/",
    "webRequest",
    "webNavigation",
    "storage",
    "downloads"
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}